 <Method>
  <ItemName>RenderNewMessageEntries</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function RenderNewMessageEntries(iLogEntryUID As Integer, sMessages() As String) As DatabaseRecord()</SourceLine>
   <SourceLine>// Created 4/7/2009 by Andrew Keller</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// generates a DatabaseRecord array for inserting a list of messages</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim result( -1 ) As DatabaseRecord</SourceLine>
   <SourceLine>Dim row, last As Integer</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>last = UBound( sMessages )</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>ReDim result( last )</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>For row = 0 To last</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>result( row ) = New DatabaseRecord</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>result( row ).IntegerColumn( kLogDB_MSG_FieldLogUID ) = iLogEntryUID</SourceLine>
   <SourceLine>result( row ).IntegerColumn( kLogDB_MSG_FieldIndex ) = row</SourceLine>
   <SourceLine>result( row ).BlobColumn( kLogDB_MSG_FieldMessage ) = sMessages( row )</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Return result</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// done.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>1</ItemFlags>
  <Shared>0</Shared>
  <ItemParams>iLogEntryUID As Integer, sMessages() As String</ItemParams>
  <ItemResult>DatabaseRecord()</ItemResult>
 </Method>
