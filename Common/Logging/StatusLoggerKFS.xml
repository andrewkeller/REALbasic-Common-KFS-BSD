  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub UnregisterQueryObject(queryObject As StatusLogQueryKFS)</SourceLine>
   <SourceLine>// Created 4/15/2009 by Andrew Keller</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// unregisters an instance of StatusLoggerKFS with this class</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim row As Integer = qi_indexOf( queryInstances, queryObject )</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>While row &gt; -1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>queryInstances.Remove row</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>row = qi_indexOf( queryInstances, queryObject )</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// done.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
