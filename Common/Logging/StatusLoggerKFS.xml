 <Method>
  <ItemName>NextPrimaryKey</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function NextPrimaryKey(tableName As String) As Integer</SourceLine>
   <SourceLine>// Created 4/15/2009 by Andrew Keller</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// returns a unique primary key value for</SourceLine>
   <SourceLine>// the given table in this database</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Static cache As New Dictionary</SourceLine>
   <SourceLine>Dim result As Integer = -1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// get old value</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If cache.HasKey( tableName ) Then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>result = cache.Value( tableName )</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If logDB.AddConnectionRequestKFS Then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim rs As RecordSet = logDB.SQLSelect( "select " + kLogDB_PL_FieldUID + " from " + kLogDB_PL_TableName + _</SourceLine>
   <SourceLine>" order by " + kLogDB_PL_FieldUID + " desc limit 1" )</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If rs &lt;&gt; Nil Then</SourceLine>
   <SourceLine>If Not rs.EOF Then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>result = rs.Field( kLogDB_PL_FieldUID ).IntegerValue</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>logDB.ReleaseConnectionRequestKFS</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// increment</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>result = result + 1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// remember</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>cache.Value( tableName ) = result</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// return result</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Return result</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// done.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <Shared>0</Shared>
  <ItemParams>tableName As String</ItemParams>
  <ItemResult>Integer</ItemResult>
 </Method>
