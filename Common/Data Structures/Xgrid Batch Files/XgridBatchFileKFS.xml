 <Method>
  <ItemName>ExportToPlist</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub ExportToPlist(plist As PropertyListKFS)</SourceLine>
   <SourceLine>// Created 5/5/2010 by Andrew Keller</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Exports the data in this class into the given plist.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If plist = Nil Then Return</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Make sure the root node is an array.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>plist.Type = PropertyListKFS.kNodeTypeArray</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Add the jobs.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim index, last As Integer</SourceLine>
   <SourceLine>last = UBound( myJobs )</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>For index = 0 To last</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>plist.Child( index ) = Job( index )</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// done.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>1</ItemFlags>
  <Shared>0</Shared>
  <ItemParams>plist As PropertyListKFS</ItemParams>
  <ItemResult></ItemResult>
 </Method>
